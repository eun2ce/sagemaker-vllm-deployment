# AWS Configuration
AWS_REGION=ap-northeast-2
AWS_ACCOUNT_ID=your-aws-account-id

# DJL Base Image (AWS Public ECR)
DJL_BASE_REGISTRY=763104351884
# 또는
DJL_BASE_IMAGE_URI=763104351884.dkr.ecr.ap-northeast-2.amazonaws.com/djl-inference:0.27.0-deepspeed0.12.6-cu121

# ECR Repository
ECR_REPOSITORY=your-model-name-sagemaker-inference
IMAGE_TAG=v0.1.0

# SageMaker Configuration
SAGEMAKER_ROLE_ARN=arn:aws:iam::your-account-id:role/your-sagemaker-role
MODEL_NAME=your-model-name
ENDPOINT_CONFIG_NAME=your-endpoint-config
ENDPOINT_NAME=your-endpoint-name

# Instance Configuration
INSTANCE_TYPE=ml.g5.xlarge
INITIAL_INSTANCE_COUNT=1

# VPC Configuration (optional)
VPC_ID=vpc-xxxxx
SUBNET_IDS=subnet-xxxxx,subnet-yyyyy
SG_IDS=sg-xxxxx